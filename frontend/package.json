{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx,ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:api": "vitest src/__tests__/api/",
    "test:components": "vitest src/__tests__/components/",
    "test:hooks": "vitest src/__tests__/hooks/",
    "test:pages": "vitest src/__tests__/pages/",
    "test:store": "vitest src/__tests__/store/",
    "test:integration": "vitest src/__tests__/integration/",
    "test:e2e": "vitest src/__tests__/e2e/",
    "test:cart": "vitest src/__tests__/hooks/useCart.test.ts src/__tests__/components/cart/ src/__tests__/pages/Cart.test.tsx",
    "test:auth": "vitest src/__tests__/api/auth.test.ts src/__tests__/hooks/useAuth.test.tsx src/__tests__/store/AuthContext.test.tsx",
    "test:checkout": "vitest src/__tests__/components/checkout/ src/__tests__/pages/Checkout.test.tsx",
    "test:products": "vitest src/__tests__/components/product/ src/__tests__/pages/Products.test.tsx",
    "test:admin": "vitest src/__tests__/components/admin/",
    "test:dashboard": "vitest src/__tests__/components/dashboard/",
    "test:subscription": "vitest src/__tests__/components/subscription/ src/__tests__/pages/Subscriptions.test.tsx",
    "test:ui": "vitest --ui",
    "test:forms": "vitest src/__tests__/components/forms/",
    "test:layout": "vitest src/__tests__/components/layout/",
    "test:generate": "node generate-tests.js",
    "test:clean": "rm -rf coverage/ && rm -rf frontend/coverage/",
    "test:ci": "vitest run --coverage --reporter=verbose",
    "test:performance": "vitest src/__tests__/performance/",
    "test:security": "vitest src/__tests__/security/",
    "test:accessibility": "vitest src/__tests__/accessibility/",
    "test:mobile": "vitest src/__tests__/mobile/",
    "test:all": "vitest run src/__tests__/",
    "test:unit": "vitest run src/__tests__/api/ src/__tests__/hooks/ src/__tests__/components/ src/__tests__/pages/ src/__tests__/store/",
    "test:integration-all": "vitest run src/__tests__/integration/",
    "test:e2e-all": "vitest run src/__tests__/e2e/",
    "test:coverage-report": "vitest run --coverage --reporter=html",
    "test:watch-unit": "vitest --watch src/__tests__/api/ src/__tests__/hooks/ src/__tests__/components/",
    "test:debug": "vitest --inspect-brk --no-coverage",
    "test:parallel": "vitest run --reporter=verbose --threads",
    "test:smoke": "vitest run src/__tests__/e2e/smoke.test.tsx",
    "test:utils": "vitest src/__tests__/utils/",
    "test:ts-files": "vitest run src/__tests__/api/ src/__tests__/hooks/ src/__tests__/utils/",
    "test:backend-aligned": "vitest run src/__tests__/api/ --reporter=verbose"
  },
  "dependencies": {
    "@greatsumini/react-facebook-login": "^3.4.0",
    "@hookform/resolvers": "^2.9.11",
    "@octokit/rest": "^20.0.2",
    "@react-oauth/google": "^0.12.2",
    "@stripe/react-stripe-js": "^2.7.0",
    "@stripe/stripe-js": "^3.3.0",
    "@tanstack/react-query": "^4.24.6",
    "@types/leaflet": "^1.9.21",
    "axios": "^1.3.4",
    "chart.js": "^4.5.0",
    "chartjs-adapter-date-fns": "^3.0.0",
    "clsx": "^2.1.1",
    "crypto-js": "^4.2.0",
    "date-fns": "^2.29.3",
    "framer-motion": "^10.18.0",
    "jsdom": "^27.0.1",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.545.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.2.0",
    "react-geolocated": "^4.4.0",
    "react-grid-layout": "^1.5.2",
    "react-hook-form": "^7.43.5",
    "react-hot-toast": "^2.6.0",
    "react-icons": "^5.5.0",
    "react-leaflet": "^4.2.1",
    "react-resizable": "^3.0.5",
    "react-router-dom": "^6.8.1",
    "recharts": "^2.5.0",
    "tailwind-merge": "^3.3.1",
    "tailwind-scrollbar-hide": "^4.0.0",
    "tailwindcss": "^3.2.7",
    "terser": "^5.44.0",
    "uuid": "^13.0.0",
    "zod": "^3.20.6"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-v8": "^4.0.2",
    "@vitest/ui": "^4.0.2",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.35.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "fast-check": "^4.3.0",
    "identity-obj-proxy": "^3.0.0",
    "msw": "^1.2.1",
    "postcss": "^8.4.21",
    "tsx": "^4.19.2",
    "typescript": "^5.9.3",
    "vite": "^4.1.0",
    "vitest": "^4.0.2"
  }
}
