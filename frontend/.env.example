# ============================================
# FRONTEND ENVIRONMENT VARIABLES (EXAMPLE)
# ============================================

# Environment Configuration
VITE_ENVIRONMENT=production

# Dynamic API URLs - Environment Specific
# Development URLs (used when VITE_ENVIRONMENT=local)
VITE_API_BASE_URL_DEV=http://localhost:8000
VITE_APP_URL_DEV=http://localhost:5173

# Production URLs (used when VITE_ENVIRONMENT=production)
VITE_API_BASE_URL_PROD=https://api.banwee.com
VITE_APP_URL_PROD=https://www.banwee.com

# Dynamic URLs (automatically set based on VITE_ENVIRONMENT)
VITE_API_BASE_URL=https://api.banwee.com
VITE_APP_URL=https://www.banwee.com

# Application Configuration
VITE_APP_NAME=Banwee
VITE_APP_DESCRIPTION="Discover premium organic products from Africa"

# Stripe Configuration
VITE_STRIPE_PUBLIC_KEY=pk_live_your_stripe_public_key

# Social Authentication
VITE_GOOGLE_CLIENT_ID=your_google_client_id
VITE_FACEBOOK_APP_ID=your_facebook_app_id

# WebSocket Configuration
VITE_WS_URL=wss://api.banwee.com/ws
VITE_WS_URL_DEV=ws://localhost:8000/ws
VITE_WS_URL_PROD=wss://api.banwee.com/ws

# Feature Flags
VITE_ENABLE_REAL_TIME_NOTIFICATIONS=true
VITE_ENABLE_CART_PERSISTENCE=true
VITE_ENABLE_ANALYTICS=true

# Analytics
VITE_GA_TRACKING_ID=G-XXXXXXXXXX
VITE_HOTJAR_ID=your_hotjar_id

# Development Settings
VITE_DEBUG_MODE=false
VITE_LOG_LEVEL=info